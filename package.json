{
  "name": "fk-infonaytto-v2",
  "version": "0.1.0",
  "description": "New version of the information display made to solve its [predecessor](https://github.com/fyysikkokilta/fk-infonaytto) problems with maintainability and perfomance. Made with React.",
  "main": "build/server.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx.ts,.tsx --fix",
    "start": "node build/server.js",
    "watch": "nodemon --watch src --exec \"ts-node\" --esm -w server.ts",
    "build:server": "tsc",
    "build:client": "webpack --mode production",
    "build": "npm run build:server && npm run build:client",
    "deploy": "npm run build && npm run start",
    "dev:client": "webpack-dev-server --mode development --open --hot",
    "dev:server": "nodemon --watch src --exec \"ts-node\" --esm server.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fyysikkokilta/fk-infonaytto-v2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fyysikkokilta/fk-infonaytto-v2/issues"
  },
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-react": "^7.22.5",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/parser": "^6.5.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "babel-loader": "^9.1.3",
    "colorthief": "^2.4.0",
    "css-loader": "^6.8.1",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "framer-motion": "^10.16.2",
    "html-webpack-plugin": "^5.5.3",
    "immer": "^9.0.21",
    "mini-css-extract-plugin": "^2.7.6",
    "moment": "^2.24.0",
    "postcss-loader": "^7.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-moment": "^1.1.3",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
